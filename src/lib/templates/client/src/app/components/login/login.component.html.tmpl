<div class="d-flex" style="width: 100vw; height: 100vh;">
  <mat-card class="col-8 col-sm-6 col-md-4 col-lg-3 col-xl-2 m-auto mat-elevation-z4">
    <mat-card-content>
      <div class="d-flex" id="login-loader">
        <svg id="NethAnimatedLogo" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
          <defs>
            <style>
              .cls-1 {
                opacity: 0.25;
              }

              .cls-2 {
                fill: #ad1457;
              }

              .cls-3 {
                fill: #c2185b;
              }

              .cls-4 {
                fill: #d81b60;
              }

              #rightBracket {
                transform-origin: center;
                transform-box: fill-box;
                transform: translate(0px) rotateZ(0deg);
              }

              #leftBracket {
                transform-origin: center;
                transform-box: fill-box;
                transform: translate(0px) rotateZ(0deg);
              }

              #slash {
                transform-origin: center;
                transform-box: fill-box;
                transform: translate(0px) rotateZ(0deg);
              }
            </style>
            <image height="96"
                   href="data:image/png;base64,"
                   id="image"
                   width="216"/>
          </defs>
          <g id="slash">
            <image class="cls-1" height="188"
                   href="data:image/png;base64,"
                   transform="translate(184 164)"
                   width="148"/>
            <polygon class="cls-2" points="323.33 168.63 237.16 315.37 188.66 343.37 274.84 196.63 323.33 168.63"/>
          </g>
          <g id="leftBracket">
            <use class="cls-1" href="#image" transform="translate(0 210)"/>
            <polygon class="cls-3" points="208 214 60 298 4 298 152 214 208 214"/>
            <image class="cls-1" height="52"
                   href="data:image/png;base64,"
                   transform="translate(0 254)"
                   width="216"/>
            <polygon class="cls-4" points="208 258 74.48 258 4 298 208 298 208 258"/>
          </g>
          <g id="rightBracket">
            <use class="cls-1" href="#image" transform="translate(300 210)"/>
            <polygon class="cls-3" points="304 298 452 214 508 214 360 298 304 298"/>
            <image class="cls-1" height="52"
                   href="data:image/png;base64,"
                   transform="translate(300 210)"
                   width="216"/>
            <polygon class="cls-4" points="304 254 437.52 254 508 214 304 214 304 254"/>
          </g>
        </svg>
      </div>
      <form #entityForm="ngForm" id="entityForm" novalidate>
        <div class="d-flex flex-wrap ">
          <mat-form-field class="w-100">
            <mat-label>Email</mat-label>
            <input [(ngModel)]="authCredentials.email" autocomplete="off" matInput name="email" required>
            <mat-icon matSuffix>email</mat-icon>
          </mat-form-field>
          <mat-form-field class="w-100">
            <mat-label>Password</mat-label>
            <input [(ngModel)]="authCredentials.password" autocomplete="off" matInput name="password" required
                   type="password">
            <mat-icon matSuffix>lock</mat-icon>
          </mat-form-field>
        </div>
        <div class="d-flex justify-content-end">
          <ng-container>
            <button (click)="login()" *ngIf="!isLoading; else loadingButton" [disabled]="entityForm.invalid"
                    color="primary" mat-raised-button>Login
            </button>
            <ng-template #loadingButton>
              <button color="accent" disabled mat-raised-button>
                <mat-spinner class="m-2" diameter="20"></mat-spinner>
              </button>
            </ng-template>
          </ng-container>
        </div>
      </form>
    </mat-card-content>
  </mat-card>
</div>
